{
  "name": "@elastic/next-eui-starter",
  "private": true,
  "description": "Start building protoypes quickly with the Next.js EUI Starter",
  "version": "1.0.0",
  "author": "Rory Hunter <rory@elastic.co>",
  "keywords": [
    "next",
    "kibana",
    "eui",
    "elastic",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn lint && rm -f public/themes/*.min.css && next build",
    "build-docs": "bash scripts/update-docs.sh",
    "dev": "next",
    "lint": "tsc && eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start",
    "test-docs": "bash scripts/test-docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/next-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/next-eui-starter/issues"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^32.1.0",
    "@next/bundle-analyzer": "^10.0.3",
    "browser-image-compression": "^1.0.14",
    "comlink": "^4.3.0",
    "glob": "^7.1.6",
    "localforage": "^1.9.0",
    "moment": "^2.29.1",
    "next": "^9.4.4",
    "next-offline": "^5.0.3",
    "next-pwa": "^5.0.6",
    "protobufjs": "^6.10.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.29.0",
    "sql.js": "^1.5.0",
    "typescript-plugin-css-modules": "^3.0.1",
    "worker-plugin": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "copy-webpack-plugin": "^6.3.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "iniparser": "^1.0.5",
    "null-loader": "^4.0.1",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.1.2"
  }
